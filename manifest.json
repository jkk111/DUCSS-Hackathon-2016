{
  "manifest_version": 2,
  "name": "Irish transport times",
  "description": "get real time infomation for public transport and traffic in dublin",
  "permissions": [
    "https://*/",
    "http://*/",
    "geolocation",
    "storage"
  ],
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
